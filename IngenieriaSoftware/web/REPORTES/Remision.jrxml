	<subDataset name="Remi" uuid="765ab6c7-9acb-4b72-aa91-229309df177c">
		<parameter name="avaluoVar" class="java.lang.String">
			<defaultValueExpression><![CDATA[]]></defaultValueExpression>
		</parameter>
		<parameter name="avaList" class="java.util.List">
			<defaultValueExpression><![CDATA[]]></defaultValueExpression>
		</parameter>
		<queryString>
			<![CDATA[select S.FK_Cod_Avaluo, S.Direccion direc, CiuAvaluo.Nombre_Ciudad,  
group_concat(ifnull(concat(C.Numero_DocCliente,' ',C.Nombre_Cliente),
concat(E.Documento_Entidad ,' ',E.Nombre_Entidad) ),' ') ClientFin,
format(S.ValorAvaluo,0) ValorAvaluo
    from seguimiento S
    left join valpac_asesor VA on S.FK_Cod_Avaluo = VA.FK_Cod_Avaluo
    left join asesor ASE on ASE.Cod_Asesor= VA.FK_Cod_Asesor
    left join sucursal Su on ASE.FK_Cod_Sucursal = Su.Cod_Sucursal
    left join entidad E on E.Cod_Entidad= Su.FK_Cod_Entidad
    left join valpaccliente VC on VC.FK_Cod_Avaluo= S.FK_Cod_Avaluo
    left join cliente C on C.Cod_Cliente = VC.FK_Cod_Cliente
    left join ciudad Ciu on Ciu.Cod_Ciudad= C.FK_Cod_Ciudad
    left join ciudad CiuEnt on CiuEnt.Cod_Ciudad= Su.FK_Cod_Ciudad
    left join ciudad CiuAvaluo on CiuAvaluo.Cod_Ciudad= S.FK_Cod_Ciudad 
    where $X{IN,S.FK_Cod_Avaluo,avaList} group by S.FK_Cod_Avaluo;]]>
		</queryString>
		<field name="FK_Cod_Avaluo" class="java.lang.Integer">
			<fieldDescription><![CDATA[Llave foranea del avaluo]]></fieldDescription>
		</field>
		<field name="direc" class="java.lang.String"/>
		<field name="Nombre_Ciudad" class="java.lang.String">
			<fieldDescription><![CDATA[Nombre del Ciudad que se va a asignar]]></fieldDescription>
		</field>
		<field name="ClientFin" class="java.lang.String"/>
		<field name="ValorAvaluo" class="java.lang.String">
			<fieldDescription><![CDATA[Valor comercial del avaluo con el que sale de la organización una vez sea facturad]]></fieldDescription>
		</field>
	</subDataset>
